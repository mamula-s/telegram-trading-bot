{
  "name": "telegram-trading-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "concurrently \"npm run watch:css\" \"nodemon src/index.js\"",
    "build:css": "tailwindcss -i ./src/webApp/styles/tailwind.css -o ./src/public/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/webApp/styles/tailwind.css -o ./src/public/styles.css --watch",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "build": "npm run build:css",
    "heroku-postbuild": "npm run migrate && npm run build",
    "build:webpack": "webpack --mode production",
    "watch:webpack": "webpack --watch --mode development"
  },
  "dependencies": {
    "@heroicons/react": "2.1.5",
    "autoprefixer": "10.4.20",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.3",
    "chart.js": "4.4.5",
    "cookie-parser": "1.4.7",
    "cron": "3.1.7",
    "crypto": "1.0.1",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "express": "4.21.1",
    "express-basic-auth": "1.2.1",
    "express-ejs-layouts": "2.5.1",
    "express-rate-limit": "5.3.0",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.453.0",
    "mongodb": "6.9.0",
    "mongoose": "8.7.0",
    "multer": "1.4.5-lts.1",
    "node-telegram-bot-api": "0.66.0",
    "nodemailer": "6.9.15",
    "pg": "8.13.1",
    "pg-hstore": "2.3.4",
    "postcss": "8.4.47",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.3",
    "recharts": "2.12.3",
    "sequelize": "6.37.5",
    "sequelize-cli": "6.6.2",
    "tailwindcss": "3.4.14"
  },
  "devDependencies": {
    "@babel/core": "7.24.0",
    "@babel/preset-react": "7.23.3",
    "babel-loader": "9.1.3",
    "concurrently": "8.2.2",
    "css-loader": "7.1.2",
    "nodemon": "3.1.7",
    "postcss-loader": "8.1.1",
    "style-loader": "4.0.0",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  },
  "engines": {
    "node": "18.x",
    "npm": "10.x"
  }
}
